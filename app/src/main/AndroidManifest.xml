<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.finfinneedesign.numbersystemconverterandcalculator">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".About"
            android:parentActivityName=".Launcher">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Launcher" />
        </activity>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Chart"
            android:parentActivityName=".Launcher">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Launcher" />
        </activity>

        <activity android:name=".Launcher" />
        <activity android:name=".Calculator"
            android:parentActivityName=".Launcher">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Launcher" />
        </activity>

        <activity android:name=".MainActivity" android:parentActivityName=".Launcher">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Launcher" />
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9464104790853930~6078932776" />
    </application>

</manifest>